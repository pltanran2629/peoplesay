<!--pages/interestingthing/interestingthing.wxml-->
<view class="cu-bar">
  <view class="action" bindtap="backtoindex">
    <text class="cuIcon-back text-gray"></text><text class="backbut">返回</text>
  </view>
</view>
<view class="container">
  <view class="main">
    <view class="main-title animation-slide-top">{{interestingthingdetail.storyName}}</view>
    <!-- <view class="main-name animation-slide-top">诗人：{{interestingthingdetail.poetName}}</view> -->
    <scroll-view scroll-y="true" style="height:1100rpx;">
    <view class="main-story animation-slide-top"><text>{{interestingthingdetail.storyDesc}}</text></view>
    </scroll-view>
  </view>
</view>